  <div class="main_container">
    {%- for block in section.blocks -%}
      {%- case block.type -%}
        {%- when 'product_item' -%}
          <div
            id="{{ block.settings.section_id }}"
            class="product_item"
            style="max-width: {{ section.settings.max_width }}px;margin: 0 auto;"
          >
            <div
              class="title_container"
              style="padding-top:{{ block.settings.padding_top }}px;padding-bottom:{{ block.settings.padding_bottom }}px;"
            >
              <h3 style="color: {{ block.settings.title_color }}">{{ block.settings.item_title }}</h3>
              {% if block.settings.title_img %}
                <img
                  src="{{ block.settings.title_img | image_url }}"
                  alt="{{ block.settings.item_title }}"
                  width="100%"
                  height="auto"
                >
              {% endif %}
            </div>
            {% if block.settings.product_img %}
              <div class="product_img_container">
                <img
                  class="pc"
                  src="{{ block.settings.product_img | image_url }}"
                  alt="{{ block.settings.product_img_alt }}"
                  width="100%"
                  height="auto"
                >
                <img
                  class="mobile"
                  src="{{ block.settings.product_img_mobile | image_url }}"
                  alt="{{ block.settings.product_img_alt }}"
                  width="100%"
                  height="auto"
                >
              </div>
            {% endif %}
            <div class="products">
              {%- if block.settings.product.title -%}
                <div
                  data-href="{% if block.settings.product_link %}{{ block.settings.product_link }}{% else %}{{ block.settings.product.url }}{% endif %}"
                  class="product_card"
                  style="background: {{ block.settings.card_bgcolor }}"
                >
                  <a href="javascript:;" class="google_button">{{ block.settings.product_link }}</a>
                  {% if block.settings.product_tag %}
                    <div
                      class="product_discount_tag"
                      style="top: calc({{ block.settings.product_tag_position_top }}% + -1%); left: {{ block.settings.product_tag_position_left }}%;"
                    >
                      <img
                        src="{{ block.settings.product_tag | image_url }}"
                        width="auto"
                        height="auto"
                        alt="{{ block.settings.product.title }}_discount"
                      >
                    </div>
                  {% endif %}
                  <div class="product_img">
                    {% if block.settings.product_image_picker != blank %}
                      <img
                        src="{{ block.settings.product_image_picker | image_url }}"
                        width="auto"
                        height="auto"
                        alt="{{ block.settings.product.title }}"
                      >
                    {% else %}
                      <img
                        src="{{ block.settings.product.featured_image | image_url }}"
                        width="auto"
                        height="auto"
                        alt="{{ block.settings.product.title }}"
                      >
                    {% endif %}
                  </div>
                  {% if block.settings.product_name == '' %}
                    <div class="product_name">{{ block.settings.product.title }}</div>
                  {% else %}
                    <div class="product_name">{{ block.settings.product_name }}</div>
                  {% endif %}
                  <div class="product_prices">
                    <div class="price_now">
                      {% if block.settings.product_price != '' %}
                        {{ block.settings.product_price | times: 100 | money }}
                      {% else %}
                        {{ block.settings.product.price | money }}
                      {% endif %}
                    </div>
                    <div class="price_max">
                      {% if block.settings.product_max_price != '' %}
                        {{ block.settings.product_max_price | times: 100 | money }}
                      {% else %}
                        {{ block.settings.product.compare_at_price | money }}
                      {% endif %}
                    </div>
                    <!--
                      <div class="learn_more">
                        {% if block.settings.product_learnMore != '' %}
                          <div class="learn_more_showmodle">
                            {{ block.settings.product_learnMore }}
                          </div>
                        {% endif %}
